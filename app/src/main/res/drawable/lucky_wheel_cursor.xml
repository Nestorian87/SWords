<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape
      android:shape="oval">
      <solid android:color="?android:windowBackground" />
      <padding
          android:top="20dp"
          android:bottom="20dp"
          android:left="20dp"
          android:right="20dp" />
      <size
        android:width="110dp"
        android:height="110dp" />
    </shape>
  </item>
  <item>
    <rotate
        android:fromDegrees="-90"
        android:toDegrees="0"
        android:drawable="@drawable/ic_left_arrow">
    </rotate>
  </item>
</layer-list>
